{
    "name": "joussin/mbc-api-content-as-package",
    "description": "Create an api content manager with backoffice",
    "license": "MIT",
    "keywords": [
        "spatie",
        "laravel-export"
    ],
    "homepage": "https://github.com/joussin/mbc-api-content-as-package",
    "authors": [
        {
            "name": "Joussin St√©phane",
            "email": "joussin@live.com",
            "homepage": "https://github.com/joussin/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",

        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",

        "illuminate/bus": "^9.18|^10.0",
        "illuminate/console": "^9.18|^10.0",
        "illuminate/database": "^9.18|^10.0",
        "illuminate/pipeline": "^9.18|^10.0",
        "illuminate/support": "^9.18|^10.0",
        "intervention/image": "^2.7",

        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/routing": "^5.4|^6.0",
        "illuminate/routing": "^9.18|^10.0",
        "illuminate/http": "^8.73|^9.0",


        "symfony/console": "^6.0",



        "illuminate/collections": "^9.18|^10.0",
        "illuminate/conditionable": "^9.18|^10.0",
        "illuminate/config": "^9.18|^10.0",

        "illuminate/container": "^9.18|^10.0",
        "illuminate/contracts": "^9.18|^10.0",
        "illuminate/cookie": "^9.18|^10.0",

        "illuminate/encryption": "^9.18|^10.0",
        "illuminate/events": "^9.18|^10.0",
        "illuminate/filesystem": "^9.18|^10.0",
        "illuminate/hashing": "^9.18|^10.0",

        "illuminate/log": "^9.18|^10.0",
        "illuminate/macroable": "^9.18|^10.0",
        "illuminate/notifications": "^9.18|^10.0",



        "illuminate/session": "^9.18|^10.0",


        "illuminate/validation": "^9.18|^10.0",
        "illuminate/view": "^9.18|^10.0"
    },
    "require-dev": {

        "ext-pdo_sqlite": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.133.11",
        "doctrine/dbal": "^2.13",
        "guzzlehttp/guzzle": "^7.4"

    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MbcApiContent\\Providers\\MainServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "MbcApiContent\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    }

}
